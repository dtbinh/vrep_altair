DEFINES += COMPILING_WITH_QT
QT       += network opengl printsupport #printsupport required from MacOS, otherwise crashes strangely ('This application failed to start because it could not find or load the Qt platform plugin "cocoa"')
greaterThan(QT_MAJOR_VERSION,4): QT += widgets
greaterThan(QT_MAJOR_VERSION,4): DEFINES += QT_DISABLE_DEPRECATED_BEFORE=0

# Compiler switches.
# Switch a few warnings off, since they are mainly generated by 3rd party code (V-REP is also still going through heavy refactoring ;) )
*-msvc* {
	QMAKE_CXXFLAGS += -O2
	QMAKE_CXXFLAGS += -W3
	QMAKE_CXXFLAGS += /wd4100 # "unreferenced formal parameter"
	QMAKE_CXXFLAGS += /wd4189 # "local variable is initialized but not referenced"
	QMAKE_CXXFLAGS += /wd4996 # "function or variable may be unsafe..."
	QMAKE_CXXFLAGS += /wd4101 # "unreferenced local variable"
}

*-g++* { #includes MinGW but not CLANG (see vrepConfig.h also)
	QMAKE_CFLAGS_RELEASE += -O3
	QMAKE_CFLAGS_RELEASE += -Wall

	QMAKE_CXXFLAGS += -O3
	QMAKE_CXXFLAGS += -Wall

	QMAKE_CFLAGS_RELEASE += -Wno-strict-aliasing
	QMAKE_CFLAGS_RELEASE += -Wno-unused-parameter
	QMAKE_CFLAGS_RELEASE += -Wno-unused-but-set-variable
	QMAKE_CFLAGS_RELEASE += -Wno-unused-local-typedefs

	QMAKE_CXXFLAGS += -Wno-unused-parameter
	QMAKE_CXXFLAGS += -Wno-strict-aliasing
	QMAKE_CXXFLAGS += -Wno-empty-body
	QMAKE_CXXFLAGS += -Wno-write-strings
	QMAKE_CXXFLAGS += -Wno-unused-but-set-variable
	QMAKE_CXXFLAGS += -Wno-unused-local-typedefs
	QMAKE_CXXFLAGS += -Wno-narrowing
}

